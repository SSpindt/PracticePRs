    "import random\n",
    "\n",
    "def dice_roll_risk():\n",
    "    yes = [1, 6]\n",
    "\n",
    "    roll = random.randint(1, 6)\n",
    "    print(f\"You rolled a {roll}.\")\n",
    "\n",
    "    if roll in yes:\n",
    "        return \"Winner number!\"\n",
    "    else:\n",
    "        return \"Uh oh! You lost!\"\n",
    "\n",
    "print(dice_roll_risk())"
